{
  "name": "ride-matcher-service",
  "version": "1.0.0",
  "description": "Intelligent ride matching service for community carpooling - designed for grace-link-jc",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "deploy": "npm run build && firebase deploy --only functions"
  },
  "keywords": [
    "ride-matching",
    "carpooling",
    "vehicle-routing",
    "community"
  ],
  "author": "Jacob",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.7.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.20",
    "@types/node": "^25.2.0",
    "@types/uuid": "^9.0.6",
    "eslint": "^8.53.0",
    "tsx": "^4.1.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "fast-xml-parser": "^5.3.4"
  }
}
